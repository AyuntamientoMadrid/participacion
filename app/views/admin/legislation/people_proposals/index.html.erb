<% provide :title do %>
  <%= t("admin.header.title") %> - <%= t("admin.menu.legislation") %> - <%= @process.title %> - <%= t("admin.legislation.people_proposals.index.title") %>
<% end %>

<div class="legislation-admin legislation-draft-versions-index">

  <%= back_link_to admin_legislation_processes_path, t("admin.legislation.people_proposals.index.back") %>

  <h2><%= @process.title %></h2>

  <%= render "admin/legislation/processes/subnav", process: @process, active: "people_proposals" %>

  <div class="small-12 medium-3">
    <%= link_to t("admin.legislation.people_proposals.index.create"), new_admin_legislation_process_people_proposal_path, class: "button" %>
  </div>
  <%= render "search_form" %>
  <%= render "people_proposals", people_proposals: @people_proposals %>
</div>
